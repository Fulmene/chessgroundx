{
  "name": "chessgroundx",
  "version": "9.0.0",
  "description": "Extended lichess.org Chess UI",
  "type": "module",
  "module": "chessground.js",
  "typings": "chessground.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-typescript": "^8",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.1",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4.2.0",
    "eslint": "^7",
    "jsdom": "^17.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.1.0",
    "prettier": "^2",
    "rollup": "^2",
    "rollup-plugin-terser": "^7",
    "ts-node": "^10.2.1",
    "tslib": "^2",
    "typescript": "^4.3"
  },
  "scripts": {
    "pretest": "npm run compile",
    "test": "mocha -r ts-node/register -r jsdom-global/register 'tests/**/*.test.ts'",
    "prepublish": "npm run compile",
    "prepare": "npm run compile",
    "compile": "tsc --outDir . --sourceMap --declaration",
    "lint": "eslint src/*.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "dist": "rollup --config"
  },
  "files": [
    "/*.js",
    "/*.d.ts",
    "/*.js.map",
    "/assets/*.css",
    "/src/*.ts",
    "!/rollup.config.js"
  ],
  "repository": "https://github.com/gbtami/chessgroundx",
  "keywords": [
    "chess",
    "lichess",
    "lichess.org",
    "chessboard",
    "ui",
    "typescript"
  ],
  "author": "Bajusz Tam√°s (https://github.com/gbtami)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gbtami/chessgroundx/issues"
  }
}
